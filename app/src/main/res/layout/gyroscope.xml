<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
   <LinearLayout
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/shake_hint_other_sensor"
        android:textSize="30sp"/><!-- Modify alert message for gsensor,msensor,gyroscope android:visibility="gone" />-->
        
    <Button
        android:id="@+id/gyroscope_cancel"
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        android:textColor="@android:color/black"/>
    <!--    android:text="@string/cancel" />-->
    <TextView
        android:id="@+id/gyroscope_result"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="30sp"/>
    </LinearLayout>
    <include
        layout="@layout/pass_fail"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"/>
</FrameLayout>
